{
    "name": "nowodev/makeservice",
    "description": "A laravel package for scaffolding service classes.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Nowodev\\Makeservice\\": "src/",
            "Nowodev\\Makeservice\\Console\\Commands\\": "src/Console/Commands/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nowodev\\Makeservice\\Tests\\": "Tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "authors": [
        {
            "name": "Nowodev",
            "email": "toluwaniokunowo@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "illuminate/console": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
        "symfony/console": "^5.4|^6.0|^7.0",
        "php": ">=8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "phpunit/phpunit": "^9.0|^10.0|^11.0|^12.0",
        "php-parallel-lint/php-parallel-lint": "dev-develop"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nowodev\\Makeservice\\MakeServiceServiceProvider"
            ]
        }
    }
}
